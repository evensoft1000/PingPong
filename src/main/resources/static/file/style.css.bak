}

/* 17 Back-to-top
----------------------------------------------------------------------------- */
.back-to-top {
    width: 40px;
    height: 40px;
    position: fixed;
    bottom: 10px;
    right: 20px;
    display: none;
    text-align: center;
    z-index: 10000;
    -webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
}
.back-to-top i {
    color: #fff;
    font-size: 15px;
    display: block;
    line-height: 33px;
}

/* 18 Responsive
----------------------------------------------------------------------------- */
@media only screen and (max-width: 767px) {
        #L_SITE_LOGO a{
		display:block;
		margin-left:15px;
		line-height:57px;
	}
	.navbar-custom {
		background-color: #fff;
	}
	.navbar-custom .navbar-brand {
		color: #333 !important;
		padding: 13px 15px 20px 15px;
		margin-top: 15px;

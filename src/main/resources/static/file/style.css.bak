	}
	.features-box .col-sm-4{
		text-align: center !important;
	}
	.advanced-about .col-sm-6:first-of-type,.skill{
		margin-bottom: 30px;
	}
	.facts {
		margin-bottom: 50px;
	}
        .footer-area{
		width:100%;
		padding:0 15px;
	}
}

